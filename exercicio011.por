/* Faça um Programa que peça dois números e imprima o maior deles
*/
programa
{
  funcao inicio()
  {
    inteiro n1, n2,  maior
 
    escreva("informe o 1º numero: ")
    leia(n1)
 
    escreva("informe o 2º numero: ")
    leia(n2)
 
    se(n1 > n2)
    {
      maior = n1
    }

    senao
    {
      maior = n2
    }
    escreva("o maior numero e: ",maior)
 
 
 
   
 
   
  }
}
 