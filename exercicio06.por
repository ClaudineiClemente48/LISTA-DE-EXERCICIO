 //Faca um programa que solicite o 'Nome' e o 'Sobrenome', exibir: 'Ola {NOME} {SOBRENOME}'
 
programa
{
 
  funcao inicio()
  {
    cadeia nome, sobrenome
 
    escreva("informe um nome: ")
    leia(nome)
 
    escreva("sobrenome: ")
    leia(sobrenome)
 
    escreva("ola " , nome, " " ,sobrenome)
    
  }
}