//Faca um Programa que peca dois numeros e imprima a multiplicacao.
programa
{
  funcao inicio()
  {
    inteiro multiplicacao, numero1, numero2
    escreva("informe o 1° numero: ")
    leia(numero1)
 
    escreva("informe 2° numero ")
    leia(numero2)
   
    multiplicacao = numero1 * numero2
    escreva("o numero multiplicado digitado e" ,multiplicacao)
   
  }
}