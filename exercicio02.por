//Efetuar a leitura de um valor numerico inteiro,
//e apresenta o seu resultadodo valor lido ao quadrado.
 
programa
{
 
  funcao inicio()
  {
    inteiro mutiplicacao, numero
 
    escreva("informe um numero ")
    leia(numero)
 
    mutiplicacao = numero  * numero
    escreva("o numero ao quadrado digitado e ",mutiplicacao)
 
 
 
   
 
 
   
  }
}